{
  "name": "react-redux-universal-hot-example",
  "description": "Example of an isomorphic (universal) webapp using react redux and hot reloading",
  "author": "Erik Rasmussen <rasmussenerik@gmail.com> (http://github.com/erikras)",
  "license": "MIT",
  "version": "0.9.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/erikras/react-redux-universal-hot-example"
  },
  "homepage": "https://github.com/erikras/react-redux-universal-hot-example",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "better-npm-run start-prod",
    "start-prod-api": "better-npm-run start-prod-api",
    "build": "better-npm-run build",
    "postinstall": "npm run build",
    "lint": "eslint -c .eslintrc src api",
    "start-dev": "better-npm-run start-dev",
    "start-dev-api": "better-npm-run start-dev-api",
    "watch-client": "better-npm-run watch-client",
    "dev-universal": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\" \"universal-redux-dev\"",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "test": "karma start",
    "test-node": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register",
    "test-node-watch": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register --watch"
  },
  "betterScripts": {
    "build": {
      "command": "universal-redux-build",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "start-prod": {
      "command": "node ./bin/start",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-prod-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./bin/start",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-dev-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "universal-redux-watch",
      "env": {
        "NODE_PATH": "./src",
        "UV_THREADPOOL_SIZE": 100
      }
    }
  },
  "dependencies": {
    "babel": "~5.8.29",
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "http-proxy": "^1.12.0",
    "piping": "^0.3.0",
    "pretty-error": "^1.2.0",
    "universal-redux": "1.0.0-beta2",
    "socket.io": "^1.3.7",
    "webpack-isomorphic-tools": "^2.2.18"
  },
  "devDependencies": {
    "babel-core": "~5.8.33",
    "babel-eslint": "^4.1.3",
    "babel-loader": "~5.3.3",
    "babel-plugin-react-transform": "~1.1.1",
    "babel-plugin-typecheck": "^2.0.0",
    "babel-runtime": "~5.8.29",
    "better-npm-run": "^0.0.4",
    "bootstrap-sass": "^3.3.5",
    "bootstrap-sass-loader": "^1.0.9",
    "chai": "^3.4.1",
    "concurrently": "^0.1.1",
    "deep-equal": "^1.0.1",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "0.1.0",
    "eslint-plugin-import": "^0.8.0",
    "eslint-plugin-react": "^3.5.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "font-awesome": "^4.4.0",
    "font-awesome-webpack": "0.0.4",
    "hoist-non-react-statics": "^1.0.3",
    "invariant": "^2.2.0",
    "karma": "^0.13.10",
    "karma-cli": "^0.1.1",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "lru-memoize": "^1.0.0",
    "mocha": "^2.3.3",
    "multireducer": "^1.0.2",
    "node-sass": "^3.4.2",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "react": "^0.14.3",
    "react-addons-test-utils": "0.14.3",
    "react-bootstrap": "^0.27.3",
    "react-document-meta": "^2.0.0",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "react-router-bootstrap": "^0.19.3",
    "react-transform-catch-errors": "^1.0.0",
    "redbox-react": "^1.1.1",
    "redux": "^3.0.4",
    "redux-form": "^3.0.0",
    "redux-simple-router": "0.0.10",
    "sinon": "^1.17.2",
    "socket.io-client": "^1.3.7",
    "superagent": "^1.4.0",
    "timekeeper": "0.0.5",
    "url-loader": "0.5.7",
    "warning": "^2.1.0"
  },
  "engines": {
    "node": "4.1.1"
  }
}
