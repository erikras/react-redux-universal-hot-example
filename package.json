{
  "name": "react-redux-universal-hot-example",
  "description": "Example of an isomorphic (universal) webapp using react redux and hot reloading",
  "author": "Erik Rasmussen <rasmussenerik@gmail.com> (http://github.com/erikras)",
  "license": "MIT",
  "version": "0.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/erikras/react-redux-universal-hot-example"
  },
  "homepage": "https://github.com/erikras/react-redux-universal-hot-example",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "node ./node_modules/better-npm-run start-prod",
    "start-prod-api": "node ./node_modules/better-npm-run start-prod-api",
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "postinstall": "webpack --display-error-details --config webpack/prod.config.js",
    "lint": "eslint -c .eslintrc src api",
    "start-dev": "node ./node_modules/better-npm-run start-dev",
    "start-dev-api": "node ./node_modules/better-npm-run start-dev-api",
    "watch-client": "node ./node_modules/better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "test": "karma start"
  },
  "betterScripts": {
    "start-prod": {
      "command": "babel-node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-prod-api": {
      "command": "babel-node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "babel-node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-dev-api": {
      "command": "babel-node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "dependencies": {
    "babel": "5.8.23",
    "babel-plugin-typecheck": "1.3.0",
    "body-parser": "1.14.1",
    "compression": "1.6.0",
    "express": "4.13.3",
    "express-session": "1.11.3",
    "file-loader": "0.8.4",
    "history": "1.12.5",
    "http-proxy": "1.11.3",
    "less": "2.5.3",
    "less-loader": "2.2.1",
    "lru-memoize": "1.0.0",
    "map-props": "1.0.0",
    "multireducer": "1.0.2",
    "piping": "0.3.0",
    "pretty-error": "1.2.0",
    "query-string": "3.0.0",
    "react": "^0.14.0",
    "react-document-meta": "2.0.0",
    "react-dom": "0.14.0",
    "react-inline-css": "2.0.0",
    "react-redux": "^4.0.0",
    "react-router": "1.0.0-rc3",
    "redux": "3.0.3",
    "redux-form": "^2.2.6",
    "serialize-javascript": "1.1.2",
    "serve-favicon": "2.3.0",
    "serve-static": "1.10.0",
    "socket.io": "1.3.7",
    "socket.io-client": "1.3.7",
    "superagent": "1.4.0",
    "url-loader": "0.5.6",
    "webpack-isomorphic-tools": "1.0.2"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.25",
    "better-npm-run": "^0.0.2",
    "bootstrap-sass": "^3.3.5",
    "bootstrap-sass-loader": "^1.0.9",
    "chai": "^3.3.0",
    "clean-webpack-plugin": "^0.1.3",
    "concurrently": "^0.1.1",
    "css-loader": "^0.20.1",
    "eslint": "1.7.3",
    "eslint-config-airbnb": "0.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-import": "^0.8.0",
    "eslint-plugin-react": "^3.5.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "font-awesome": "^4.4.0",
    "font-awesome-webpack": "0.0.4",
    "json-loader": "^0.5.3",
    "karma": "^0.13.10",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "^0.1.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "node-sass": "^3.3.3",
    "react-a11y": "^0.2.6",
    "react-addons-test-utils": "^0.14.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "redux-router": "1.0.0-beta3",
    "sass-loader": "^3.0.0",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1"
  },
  "engines": {
    "node": "4.1.1"
  }
}
